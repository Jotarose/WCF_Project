<Window x:Class="MathClientWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MathClientWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <Label Content="Número:" Grid.Row="1" FontSize="20" Margin="90,130,607,-151"/>
        <TextBox Grid.Row="1" FontSize="22" Margin="215,132,31,-149" x:Name="Valor"/>

        <Label Content="Resultado:" Grid.Row="1" FontSize="20" Margin="90,248,607,-274"/>
        <TextBox Grid.Row="1" FontSize="22" Margin="215,248,31,-261" x:Name="Resultado" IsReadOnly="True"/>

        <Button Content="Primo" FontSize="18" Grid.Row="1" Click="Button_Click" Margin="659,325,31,-325"/>
    </Grid>
</Window>
